  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "repository": {
    "url": "https://github.com/rocket-libs/Potter-Nano-Framework",
    "type": "git"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@types/jest": "^25.2.3",
    "@types/styled-components": "^4.4.0",
    "concurrently": "^5.0.0",
    "install-peers-cli": "^2.1.1",
    "jest": "^26.0.1",
    "prepend-file": "^1.3.1",
    "rollup": "^1.27.5",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.5.2",
    "tslib": "^1.10.0",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {},
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "start-example": "cd example && npm start",
    "deploy-example": "cd example && npm run deploy",
    "dev": "concurrently --kill-others \"npm run build-watch\" \"npm run start-example\"",
    "test": "ts-node test/test.ts",
    "pretest": "yarn run build",
    "test:lint": "eslint ./src/**/*.ts ./src/**/*.tsx ./example/src/**/*.ts ./example/src/**/*.tsx",
    "test:lint:fix": "yarn run test:lint -- --fix"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "shuttlerjs": "^1.1.9"
  }
